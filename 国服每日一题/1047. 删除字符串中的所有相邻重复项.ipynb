   "source": [
    "class Solution:\n",
    "    def removeDuplicates(self, S: str) -> str:\n",
    "        stack = []\n",
    "        for v in S:\n",
    "            if stack and stack[-1] == v:\n",
    "                stack.pop()\n",
    "            else:\n",
    "                stack.append(v)\n",
    "        return ''.join(stack)"
   ]
  }
