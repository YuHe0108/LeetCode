   "source": [
    "class Solution:\n",
    "    def replaceDigits(self, s: str) -> str:\n",
    "        s_list = list(s)\n",
    "        \n",
    "        for i, v in enumerate(s_list):\n",
    "            if i % 2 == 1:\n",
    "                s_list[i] = chr(ord(s_list[i-1]) + int(v))\n",
    "        \n",
    "        return ''.join(s_list)"
   ]
  }
