   "source": [
    "class Solution:\n",
    "    def getMinDistance(self, nums: List[int], target: int, start: int) -> int:\n",
    "        \n",
    "        ans = float('inf')\n",
    "        for i, v in enumerate(nums):\n",
    "            if v == target:\n",
    "                ans = min(ans, abs(i - start))\n",
    "        return ans\n",
    "                \n"
   ]
  }
