   "source": [
    "class Solution:\n",
    "    def sumBase(self, n: int, k: int) -> int:\n",
    "        bit_vals = []\n",
    "        while n:\n",
    "            bit_vals.append(n % k)\n",
    "            n //= k\n",
    "        return sum(bit_vals)"
   ]
  },
