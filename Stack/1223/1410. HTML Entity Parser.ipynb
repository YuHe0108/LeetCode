   "source": [
    "import html\n",
    "class Solution:\n",
    "    def entityParser(self, text: str) -> str:\n",
    "        text = text.replace('&frasl;','/')\n",
    "        return html.unescape(text)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "solution = Solution()\n",
    "solution.entityParser(text = \"&amp; is an HTML entity but &ambassador; is not.\")"
   ]
