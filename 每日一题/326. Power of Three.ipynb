   "source": [
    "class Solution:\n",
    "    def isPowerOfThree(self, n: int) -> bool:\n",
    "        while n > 1:\n",
    "            if n % 3 == 0:\n",
    "                n //= 3\n",
    "            else:\n",
    "                return False\n",
    "        return n == 1"
   ]
