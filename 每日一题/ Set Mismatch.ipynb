   "source": [
    "class Solution:\n",
    "    def findErrorNums(self, nums: List[int]) -> List[int]:\n",
    "        now_sum = sum(set(nums))\n",
    "        ori_sum = sum(set([x for x in range(len(nums) + 1)]))\n",
    "        err_sum = sum(nums) # 多算了重复的数字\n",
    "        return [err_sum - now_sum, ori_sum - now_sum]"
   ]
  }
