   "source": [
    "class Solution:\n",
    "    def hammingWeight(self, n: int) -> int:\n",
    "        cnt = 0\n",
    "        while n:\n",
    "            if n & 1 == 1:\n",
    "                cnt += 1\n",
    "            n >>= 1\n",
    "        return cnt"
   ]
  },
