   "source": [
    "class Solution:\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "        s_list = s.split(' ')\n",
    "        res = [w[::-1] for w in s_list]\n",
    "        return ' '.join(res)"
   ]
