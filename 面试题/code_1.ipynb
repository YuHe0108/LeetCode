    "def solution_bottle():\n",
    "    bot = 10\n",
    "    cnt = 0\n",
    "    while bot > 3:\n",
    "        print(bot)\n",
    "        v = bot // 3\n",
    "        cnt += v\n",
    "        bot = bot - v * 3 + v\n",
    "        \n",
    "    return cnt"
   ]
